    <client>
      <endpoint address="http://localhost:50491/wsNew.asmx" binding="basicHttpBinding"
        bindingConfiguration="wsNewSoap" contract="ServiceReference1.wsNewSoap"
        name="wsNewSoap" />
    </client>
  </system.serviceModel>
